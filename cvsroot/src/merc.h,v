    sh_int		mutant_timer; 
    sh_int		skill_point_timer; 
    sh_int		skill_point_tracker; 
    sh_int		logout_tracker; 
    sh_int		gladiator_attack_timer; 
    sh_int		gladiator_team; 
    sh_int		savant;
    int                 last_level;
    sh_int              condition       [4];
