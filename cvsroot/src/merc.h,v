    LINE_EDIT_DATA *    line_edit;
    CHAR_DATA *		req_trade;
    bool                no_out;
    char *		email;
    sh_int              macro_count;
    sh_int		node;
    sh_int		capped;
    sh_int		specialize;
    sh_int		old_class;
    bool                valid;
    char *              pwd;
    char *              bamfin;
    char *              bamfout;
    char *              title;
    char *              who_name;
    char *              last_kill;
    char *		last_attacked_by;
